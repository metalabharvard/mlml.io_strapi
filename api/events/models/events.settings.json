{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "events",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "start_time": {
      "type": "datetime"
    },
    "location": {
      "type": "string"
    },
    "link": {
      "type": "string"
    },
    "intro": {
      "type": "text"
    },
    "description": {
      "type": "richtext"
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    },
    "members": {
      "via": "events",
      "collection": "member",
      "dominant": true
    },
    "projects": {
      "via": "events",
      "collection": "projects",
      "dominant": true
    },
    "cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "gallery": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "isOngoing": {
      "type": "boolean"
    },
    "Timezone": {
      "type": "enumeration",
      "enum": [
        "Boston",
        "Berlin"
      ],
      "default": "Boston"
    },
    "end_time": {
      "type": "datetime"
    },
    "youtube": {
      "type": "string"
    },
    "vimeo": {
      "type": "string"
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "component": "link.links"
    },
    "appendix": {
      "type": "richtext"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "seminar",
        "workshop",
        "opening"
      ]
    },
    "topics": {
      "via": "events",
      "collection": "topics",
      "dominant": true
    },
    "collaborators": {
      "type": "component",
      "repeatable": true,
      "component": "collabortor.collaborators"
    },
    "press_articles": {
      "type": "component",
      "repeatable": true,
      "component": "press-link.press"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "teaching",
        "event"
      ]
    }
  }
}
