{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "projects",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "intro": {
      "type": "text"
    },
    "start": {
      "type": "date"
    },
    "end": {
      "type": "date"
    },
    "externalLink": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "isFeatured": {
      "type": "boolean"
    },
    "events": {
      "via": "projects",
      "collection": "events"
    },
    "members": {
      "via": "projects",
      "collection": "member",
      "dominant": true
    },
    "cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "gallery": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "title"
    },
    "collaborators": {
      "type": "component",
      "repeatable": true,
      "component": "collabortor.collaborators"
    },
    "press_articles": {
      "type": "component",
      "repeatable": true,
      "component": "press-link.press"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "project",
        "publication"
      ],
      "default": "project"
    },
    "host": {
      "type": "enumeration",
      "enum": [
        "harvard",
        "berlin",
        "both"
      ],
      "default": "harvard"
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "component": "link.links"
    },
    "location": {
      "type": "string"
    },
    "topics": {
      "collection": "topics",
      "via": "topic"
    }
  }
}
